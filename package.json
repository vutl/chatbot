{
  "name": "stock-ai-agent",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "backend:build": "nest build",
    "backend:format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "backend:start": "nest start",
    "backend:dev": "nest start --watch",
    "backend:debug": "nest start --debug --watch",
    "backend:prod": "node dist/main",
    "backend:lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "backend:test": "jest",
    "backend:test:watch": "jest --watch",
    "backend:test:cov": "jest --coverage",
    "backend:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "backend:test:e2e": "jest --config ./test/jest-e2e.json",
    "client:dev": "cd web-client && npm run dev",
    "client:build": "cd web-client && npm run build",
    "client:build:dev": "cd web-client && npm run build:dev",
    "client:build:prod": "cd web-client && npm run build:prod",
    "client:preview": "cd web-client && npm run preview",
    "dev": "npx concurrently \"npm run backend:dev\" \"npm run client:dev\"",
    "build:dev": "npx concurrently \"npm run backend:build\" \"npm run client:build:dev\"",
    "build:prod": "npx concurrently \"npm run backend:build\" \"npm run client:build:prod\"",
    "install:all": "npm install && cd web-client && npm install",
    "start:zenai": "nest start --config nest-cli.zenai.json",
    "start:zenai:dev": "nest start --config nest-cli.zenai.json --watch",
    "start:zenai:prod": "node dist/zenai-stock.main",
    "build:zenai": "nest build --config nest-cli.zenai.json",
    "build:start:zenai:prod": "npm run build:zenai && pm2 delete zenai-AI-stock-processor || true && pm2 start dist/zenai-stock.main.js --name zenai-AI-stock-processor"
  },
  "dependencies": {
    "@langchain/openai": "^0.4.4",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.10",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.0.3",
    "@types/axios": "^0.9.36",
    "@types/multer": "^1.4.12",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.7.9",
    "bull": "^4.16.5",
    "chromadb": "^1.10.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.7",
    "form-data": "^4.0.2",
    "html-to-text": "^9.0.5",
    "langchain": "^0.3.19",
    "mammoth": "^1.9.0",
    "markdown-it": "^14.1.0",
    "openai": "^4.85.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "rtf-parser": "^1.3.3",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.1.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
